<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<!--
    Configuration file for UrlRewriteFilter
    http://tuckey.org/urlrewrite/
-->

<urlrewrite>
   
    <rule>
        <from>login</from>
        <to type="forward">login.jsp</to>
    </rule>
    <rule>
        <from>lefee</from>
        <to type="forward">home.jsp</to>
    </rule>
    <rule>
        <from>businessmanLogin</from>
        <to type="forward">businessmanLogin.jsp</to>
    </rule>
    
    <!-- <rule>
        <from>^/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/(\w+)/(\d+)</from>
        <to type="forward">/store/$1_$2.action?$3=$4</to>
    </rule> -->
    <rule>
        <from>^/(store|core|business|dish)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/(\w+)/(\d+)\.php$</from>
        <to type="forward">/$1/$2_$3.action?$4=$5</to>
    </rule>
    <rule>
        <from>^/(store|core|business|dish)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)\.php$</from>
        <to type="forward">/$1/$2_$3.action</to>
    </rule>
    <rule>
        <from>^/(store|core|business|dish)/([a-zA-Z0-9]+)\.php$</from>
        <to type="forward">/$1/$2.action</to>
    </rule>
</urlrewrite>
